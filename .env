# Variables de connexion à la base de données PostgreSQL
DB_HOST='' # L'adresse du serveur de la base de données
DB_PORT='' # Le port du serveur de la base de données (5432 est le port par défaut de PostgreSQL)
DB_NAME='' # Le nom de la base de données
DB_USERNAME='' # Le nom d'utilisateur pour se connecter à la base de données
DB_PASSWORD='' # Le mot de passe pour se connecter à la base de données

# Variables pour la configuration du client S3
S3_ACCESS_KEY_ID='' # Votre ID de clé d'accès AWS
S3_SECRET_ACCESS_KEY='' # Votre clé d'accès secrète AWS
S3_REGION='' # La région de votre bucket S3 (peut être une valeur arbitraire pour les solutions S3 non AWS)
S3_BUCKET_NAME='' # Le nom de votre bucket S3
S3_ENDPOINT_URL='' # L'URL de l'endpoint de votre solution S3

# Variables pour le chiffrement GPG
GPG_PASSPHRASE='' # La passphrase pour chiffrer les dumps avec GPG
